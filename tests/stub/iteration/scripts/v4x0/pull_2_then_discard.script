    S: RECORD ["1"]
       RECORD ["2"]
       SUCCESS {"has_more": true}
?}
C: DISCARD {"n": -1}
S: SUCCESS {"type": "#MODE#"}
{*
    C: RESET
    S: SUCCESS {}
*}
